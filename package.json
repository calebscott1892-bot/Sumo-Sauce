{
  "name": "sumowatch",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:all": "concurrently -k -n server,web \"npm run server:dev\" \"npm run dev\"",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "bootstrap": "npm run server:db:push && npm run server:bootstrap",
    "api:dev": "node -e \"console.log('API base URL: http://127.0.0.1:8790')\" && npm run server:dev",
    "server:dev": "PORT=8790 ADMIN_TOKEN=dev-token npm --prefix server run dev",
    "server:dev:8787": "PORT=8787 ADMIN_TOKEN=dev-token npm --prefix server run dev",
    "server:db:push": "npm --prefix server run db:push",
    "server:bootstrap": "npm --prefix server run bootstrap",
    "verify:backend-contract": "node scripts/verify-backend-contract.mjs",
    "api:smoke": "node scripts/api-smoke-test.mjs",
    "e2e:smoke": "node scripts/e2e-smoke.mjs",
    "import:dry:wrestlers": "node scripts/import-json-chunks.mjs --entity Wrestler --file ./data/wrestlers_final.json --server http://127.0.0.1:8787 --token dev-token --chunk 300 --dry-run 1",
    "import:wrestlers": "node scripts/import-json-chunks.mjs --entity Wrestler --file ./data/wrestlers_final.json --server http://127.0.0.1:8787 --token dev-token --chunk 300 --dry-run 0",
    "import:dry:basho": "node scripts/import-json-chunks.mjs --entity BashoRecord --file ./data/basho_records_final.json --server http://127.0.0.1:8787 --token dev-token --chunk 300 --dry-run 1",
    "import:basho": "node scripts/import-json-chunks.mjs --entity BashoRecord --file ./data/basho_records_final.json --server http://127.0.0.1:8787 --token dev-token --chunk 300 --dry-run 0",
    "merge:wrestlers": "node scripts/merge-wrestlers-from-basho.mjs",
    "import:all": "node -e \"if(!process.env.ADMIN_TOKEN){console.error('ADMIN_TOKEN is required');process.exit(1)}\" && node scripts/run-full-import.mjs",
    "backfill:images": "node scripts/backfill-wrestler-images.mjs",
    "ingest:wrestlers:enrich": "node scripts/ingest-wrestlers-enriched.mjs",
    "pipeline:build": "node --experimental-strip-types pipeline/build.ts",
    "pipeline:verify": "node --experimental-strip-types pipeline/verify.ts",
    "pipeline:diff": "node --experimental-strip-types pipeline/db/diff.ts",
    "pipeline:load-db": "node --experimental-strip-types pipeline/db/load.ts",
    "pipeline:run": "node scripts/pipeline-load-db.mjs",
    "verify:pipeline-noop": "node scripts/pipeline-run-noop-check.mjs",
    "hash:stability-check": "node --experimental-strip-types scripts/hash-stability-check.mjs",
    "fixtures:verify": "node --experimental-strip-types scripts/fixtures-verify.mjs",
    "fixtures:verify-range": "node --experimental-strip-types scripts/fixtures-verify-range.mjs",
    "ingest:test": "node --experimental-strip-types scripts/ingest-test.mjs",
    "capture:range": "node --experimental-strip-types scripts/capture-range.mjs",
    "ingest:offline-range": "node --experimental-strip-types scripts/ingest-offline-range.mjs",
    "verify:safe-range": "node scripts/safe-range-check.mjs",
    "verify:domain-api": "node scripts/verify-domain-api.mjs",
    "test:basho-range": "node --experimental-strip-types scripts/test-basho-range.mjs",
    "ingest": "node --experimental-strip-types scripts/ingest-range.mjs"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.56.1",
    "@hookform/resolvers": "^4.1.2",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-aspect-ratio": "^1.1.2",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-context-menu": "^2.2.6",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-hover-card": "^1.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toggle": "^1.1.2",
    "@radix-ui/react-toggle-group": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@tanstack/react-query": "^5.90.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cheerio": "^1.1.2",
    "cmdk": "^1.0.0",
    "date-fns": "^3.6.0",
    "embla-carousel-react": "^8.5.2",
    "framer-motion": "^12.4.7",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.475.0",
    "next-themes": "^0.4.4",
    "react": "18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.54.2",
    "react-resizable-panels": "^2.1.7",
    "react-router-dom": "^7.2.0",
    "recharts": "^2.15.1",
    "sonner": "^2.0.1",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@flydotio/dockerfile": "^0.7.8",
    "@types/node": "^22.13.5",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.1",
    "eslint": "^9.19.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "playwright": "^1.57.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "vite": "^6.1.0"
  }
}
